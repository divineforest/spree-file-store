<h1><%= t('upload_a_new_file') %></h1>

<% form_for(:uploaded_file, :url => { :action => "create" }, :html => { :multipart => true }) do |form| %>
  <fieldset>
    <%= form.file_field :asset %><br />
    <div class="upload_button">
      <%= button t('upload_file') %>
    </div>
  </fieldset>
<% end %>